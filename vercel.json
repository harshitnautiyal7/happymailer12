{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/login",
      "destination": "/api/index.js"
    },
    {
      "source": "/send",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/Public/$1"
    }
  ]
}